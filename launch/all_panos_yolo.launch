<launch>
  <arg name="namespace" default="ben"/>
  <arg name="image_topic" default="image_raw"/>

  <include file="$(find ben_hardware)/launch/pano_yolo.launch"><arg name="camera_number" value="1"/>
    <arg name="namespace" value="$(arg namespace)"/>
    <arg name="image_topic" value="$(arg image_topic)"/>
  </include>
  <include file="$(find ben_hardware)/launch/pano_yolo.launch"><arg name="camera_number" value="2"/>
    <arg name="namespace" value="$(arg namespace)"/>
    <arg name="image_topic" value="$(arg image_topic)"/>
  </include>
  <include file="$(find ben_hardware)/launch/pano_yolo.launch"><arg name="camera_number" value="3"/>
    <arg name="namespace" value="$(arg namespace)"/>
    <arg name="image_topic" value="$(arg image_topic)"/>
  </include>
  <include file="$(find ben_hardware)/launch/pano_yolo.launch"><arg name="camera_number" value="4"/>
    <arg name="namespace" value="$(arg namespace)"/>
    <arg name="image_topic" value="$(arg image_topic)"/>
  </include>
  <include file="$(find ben_hardware)/launch/pano_yolo.launch"><arg name="camera_number" value="5"/>
    <arg name="namespace" value="$(arg namespace)"/>
    <arg name="image_topic" value="$(arg image_topic)"/>
  </include>
  <include file="$(find ben_hardware)/launch/pano_yolo.launch"><arg name="camera_number" value="6"/>
    <arg name="namespace" value="$(arg namespace)"/>
    <arg name="image_topic" value="$(arg image_topic)"/>
  </include>
</launch>
